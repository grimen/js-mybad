{
    "extends": [
        "eslint:recommended"
    ],
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "node": true
    },
    "globals": {
        "Promise": true
    },
    "rules": {
        "no-unused-vars": 1,
        "no-console": 0,
        "no-inner-declarations": 0
    }
}
